# Yap Configuration
# Copy this file to .env and customize as needed

CADDY_NETWORK=caddy

# Domains (set these to your real hostnames)
ASR_DOMAIN=asr.localhost
TTS_DOMAIN=tts.localhost

# Host paths for persistence
WHISPER_MODELS_PATH=/srv/yap/asr/models
PIPER_MODELS_PATH=/srv/yap/tts/models

# ASR model defaults
ASR_ENGINE=faster_whisper
ASR_MODEL=tiny.en
ASR_DEVICE=cuda
ASR_MODEL_PATH=/models
