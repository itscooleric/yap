# YAP Exporter Configuration
# Copy to .env and fill in your values

# GitLab
GITLAB_URL=https://gitlab.com
GITLAB_TOKEN=

# GitHub
GITHUB_API_URL=https://api.github.com
GITHUB_TOKEN=

# Ollama (optional, for generating commit messages)
OLLAMA_BASE_URL=
OLLAMA_MODEL=llama3

# SFTP (optional)
SFTP_HOST=
SFTP_PORT=22
SFTP_USER=
SFTP_PASSWORD=
SFTP_KEY_PATH=
SFTP_BASE_PATH=/uploads

# Default export settings
EXPORT_DEFAULT_REPO=
EXPORT_DEFAULT_BRANCH=main
